html,
body {
	height: 100%;
	margin: 0;
	padding: 0;
	width: 100%;
}

body {
	width: 100vw;
	height: 100vh;
	overflow: hidden;
	background: #000;
}

.map-scroll-outer {
	width: 100vw;
	height: 100vh;
	overflow: hidden;
	position: fixed;
	top: 0;
	left: 0;
	background: #000;
}

.map-container {
	width: 100vw;
	height: 100vh;
	position: relative;
	background: #000;
	overflow: hidden;
	cursor: grab; /* добавлено */
}

#markerContainer {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	pointer-events: none;
	transform-origin: 0 0; /* изменено с 50% 50% */
}

.full-map {
	width: 100vw;
	height: 100vh;
	object-fit: contain;
	display: block;
	margin: 0;
	padding: 0;
	user-select: none;
	-webkit-user-drag: none;
	position: absolute;
	top: 0;
	left: 0;
	cursor: grab; /* изменено с crosshair */
	transform-origin: 0 0; /* добавлено */
}

.event-modal {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.7);
	z-index: 1000;
	justify-content: center;
	align-items: center;
}

.event-modal.active {
	display: flex;
}

.event-form {
	display: flex;
	flex-direction: column;
	gap: 10px;
	background: white;
	padding: 20px;
	border-radius: 8px;
	min-width: 300px;
	max-width: 500px;
}

.event-form input,
.event-form select,
.event-form textarea {
	padding: 8px;
	border: 1px solid #ccc;
	border-radius: 4px;
}

.event-form button {
	padding: 8px 16px;
	background: #4caf50;
	color: white;
	border: none;
	border-radius: 4px;
	cursor: pointer;
	margin-right: 8px;
}

.event-form button[type='button'] {
	background: #f44336;
}

.event-form button:hover {
	opacity: 0.9;
}

.event-marker {
	position: absolute;
	width: 24px;
	height: 24px;
	transform: translate(-50%, -50%);
	cursor: pointer;
	z-index: 100;
	background-size: contain;
	background-repeat: no-repeat;
	background-position: center;
	pointer-events: auto;
}


.event-details-modal {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.7);
	z-index: 1000;
	justify-content: center;
	align-items: center;
}

.event-details-modal.active {
	display: flex;
}

.event-details-content {
	background: white;
	padding: 20px;
	border-radius: 8px;
	min-width: 300px;
	max-width: 500px;
}

.event-details-content button {
	width: 100%;
	padding: 10px;
	background: #4caf50;
	color: white;
	border: none;
	border-radius: 4px;
	cursor: pointer;
	margin-top: 10px;
}

.admin-modal {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.5);
	z-index: 1002;
	justify-content: center;
	align-items: center;
	backdrop-filter: blur(2px);
}

.admin-modal.active {
	display: flex;
}

.admin-modal-content {
	background: white;
	padding: 24px;
	border-radius: 12px;
	min-width: 400px;
	max-width: 600px;
	box-shadow: 0 4px 20px rgba(0, 0, 0, 0.15);
	max-height: 80vh; /* Максимальная высота самого модального окна */
	overflow-y: auto;
}

.admin-modal h3 {
	margin: 0 0 20px 0;
	text-align: center;
	font-size: 1.5em;
	color: #333;
}

.admin-modal input {
	width: 100%;
	padding: 12px;
	margin: 8px 0 20px;
	border: 1px solid #ddd;
	border-radius: 6px;
	font-size: 14px;
}

.button-group {
	display: flex;
	gap: 10px;
}

.button-group button {
	flex: 1;
}

.primary-button {
	background: #4caf50;
	color: white;
	border: none;
	padding: 12px 24px;
	border-radius: 6px;
